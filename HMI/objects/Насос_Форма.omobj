			<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="2" event="FocusChanged">
				<body kind="om">
					<![CDATA[// При нажатии на кнопку,

if (me.Focus)
{

// если уровень доступа "Инженер" и выше, то:

	if (unit.Security.Engineer.Value)
	{
		if (unit.Global.bool_input.Value)
		{

// 1) Если значение введённое значение таймаута на пуск отличается от текущего, то изменить его и выдать соответствующее сообщение

			if (Start_Timeout != Str.ToUint2(TextEdit_CloseTimeout.Text, 9999))
				{
				unit.Global.MessagesOper.Value="<Subcondition Message='"+ Description + ". Изменить значение таймаута на закрытие на " + TextEdit_CloseTimeout.Text + "). [Пользователь: " + unit.Security.SecurityContext.CurrentUser + ". АРМ: " + unit.Global.Network.ComputerName + "] ' Value='' Sound='' Severity='101' Enabled='1' SoundEnabled='0' />";
				Start_Timeout = Str.ToUint2(TextEdit_CloseTimeout.Text, 9999);
				}	
			
// 2) Если значение введённое значение таймаута на останов отличается от текущего, то изменить его и выдать соответствующее сообщение			
				
			if (Stop_Timeout != Str.ToUint2(TextEdit_OpenTimeout.Text, 9999))
				{
				unit.Global.MessagesOper.Value="<Subcondition Message='"+ Description + ". Изменить значение таймаута на открытие на " + TextEdit_OpenTimeout.Text + "). [Пользователь: " + unit.Security.SecurityContext.CurrentUser + ". АРМ: " + unit.Global.Network.ComputerName + "] ' Value='' Sound='' Severity='101' Enabled='1' SoundEnabled='0' />";
				Stop_Timeout = Str.ToUint2(TextEdit_OpenTimeout.Text, 9999);
				}	

// 3) завершить выполнение кода			

			unit.Global.bool_input.Value = false; //сброс флага ввода
		}
	}
}]]>
				</body>
