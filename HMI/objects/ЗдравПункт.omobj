// 2) установить соответствующие переменные для отправки команд (с ВУ)

			P1_Start = true;
			P1_Stop  = false;

// 3) завершить выполнение кода			
			
			unit.Global.bool_input.Value = false; //сброс флага ввода
		}
	}
}]]>
					</body>
